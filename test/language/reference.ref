include stdio

int set_int(ref int i)
{
	i = 42
	return i
}

ref int set_int_to(ref int i, ref int j)
{
	i = j
	return ref i
}

ref int set_int_ref(ref int i, ref int j)
{
	ref i = ref j
	return ref i
}

int main()
{
	int i
	ref int j = 21

	i = set_int(ref i)
	ref j = ref set_int_to(ref i, ref j)
	j = set_int_ref(ref i, ref j)

	return 0
}
